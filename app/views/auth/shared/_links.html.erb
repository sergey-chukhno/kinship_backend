<div class="form__links-wrapper">
  <%- if controller_name != 'sessions' %>
    <p class="xs-paragraph dark-01">Déja inscrit, <%= link_to "se connecter", new_session_path(resource_name) %></p>
  <% end %>
  <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
    <p class="xs-paragraph dark-01">S'inscrire en tant que <%= link_to "parent", new_registration_stepper_first_step_path(role: "tutor") %>, <%= link_to "enseignant", new_registration_stepper_first_step_path(role: "teacher") %>, <%= link_to "volontaire", new_registration_stepper_first_step_path(role: "voluntary") %> ou <%= link_to "organisation", new_registration_stepper_first_step_path(role: "voluntary", company_form: true) %></p>
  <% end %>
  <%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
    <%= link_to "Mot de passe oublié?", new_password_path(resource_name), class: "xs-paragraph" %>
  <% end %>
</div>
